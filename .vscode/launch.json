    "version": "0.2.0",
    "configurations": [
    {
        "name": "C/C++: g++ VisionProj",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/bin/VisionProj", //程序文件路径
        "args": [], //程序运行需传入的参数
        "stopAtEntry": false,
        "cwd": "${fileDirname}",
        "environment": [],
        "externalConsole": false, //运行时是否显示控制台窗口
        "MIMode": "gdb",
        "setupCommands": [
        {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
        }
        ],
        "preLaunchTask": "C/C++: g++ VisionProj",
        "miDebuggerPath": "/usr/bin/gdb"
    }
 ]
}